	  esac; \
	done; \
	echo ' cd $(top_srcdir) && $(AUTOMAKE) --gnu  spec/main/Makefile'; \
	cd $(top_srcdir) && \
	  $(AUTOMAKE) --gnu  spec/main/Makefile
